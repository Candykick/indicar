<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="1dp" android:top="1dp">
        <shape>
            <solid android:color="@color/transShadow"/>
            <stroke android:color="@color/transShadow" android:width="1dp"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:right="1dp" android:bottom="1dp">
        <shape>
            <solid android:color="@color/transWhite"/>
            <stroke android:color="@color/transShadow" android:width="1dp"/>
            <corners android:radius="5dp"/>
            <padding android:left="10dp" android:right="10dp" android:top="10dp" android:bottom="10dp"/>
        </shape>
    </item>
</layer-list>